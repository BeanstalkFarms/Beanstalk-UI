  "scripts": {
    "start": "vite --port 4173",
    "dev": "netlify dev --command \"yarn start\" --targetPort 4173",
    "serve": "vite preview",
    "build": "yarn generate && tsc && vite build",
    "build-netlify": "VITE_NAME=$npm_package_name VITE_VERSION=$npm_package_version VITE_HOST='netlify' VITE_NETLIFY_CONTEXT=$CONTEXT VITE_NETLIFY_BUILD_ID=$BUILD_ID VITE_GIT_COMMIT_REF=$COMMIT_REF yarn build",
    "build-fleek": "VITE_NAME=$npm_package_name VITE_VERSION=$npm_package_version VITE_HOST='fleek' VITE_GIT_COMMIT_REF=$(git rev-parse HEAD) yarn build",
    "build:vite": "vite build",
    "test": "yarn run vitest",
    "clean": "rm -rf ./src/generated && echo \"Cleaned src/generated.\"",
    "lint": "eslint \"src/**/**/*.ts{,x}\"",
    "format": "prettier --write \"**/*.{ts,tsx,json,md,js}\"",
    "generate": "yarn clean && yarn generate:typechain && yarn generate:graphql && echo \"Done generating files.\"",
    "generate:typechain": "echo \"Generating: typechain ethers-v5\" && typechain --target=ethers-v5 --ts-nocheck --out-dir=./src/generated './src/constants/abi/**/*.json'",
    "generate:graphql": "echo \"Generating: graphql-codegen\" && graphql-codegen --config codegen.yml",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "postpack": "yarn generate",
    "postinstall": "husky install"
  },
  "engines": {
    "node": ">=14"
  },
  "lint-staged": {
    "./src/**/*.{ts,tsx,json}": "eslint --fix"
  },
  "packageManager": "yarn@3.2.1",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
