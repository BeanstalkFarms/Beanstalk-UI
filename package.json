    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "react-app-rewired": "^2.2.1",
    "react-error-overlay": "6.0.9",
    "typechain": "^8.0.0",
    "webpack-cli": "^4.9.2"
  },
  "resolutions": {
    "react-error-overlay": "6.0.9",
    "babel-loader": "8.1.0"
  },
  "scripts": {
    "start": "node --max_old_space_size=4096 node_modules/.bin/react-app-rewired start",
    "build": "yarn generate && node --optimize_for_size --max_old_space_size=8192 node_modules/.bin/react-app-rewired build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test": "react-scripts-rewired test",
    "eject": "react-scripts-rewired eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "format": "prettier --write \"**/*.{ts,tsx,json,md,js}\"",
    "lint": "eslint \"src/**/**/*.ts{,x}\"",
    "generate": "typechain --target=ethers-v5 --ts-nocheck --out-dir=./src/constants/generated './src/constants/abi/**/*.json'",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "lint-staged": {
    "*.{ts,tsx,json}": "prettier --write"
  },
  "engines": {
    "node": ">=14"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
